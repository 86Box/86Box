#  endif
#  ifdef ENABLE_LOG_BREAKPOINT
        MENUITEM "&Log breakpoint\tCtrl+F10", IDM_LOG_BREAKPOINT
#  endif
#  ifdef ENABLE_VRAM_DUMP
        MENUITEM "Dump &video RAM\tCtrl+F1", IDM_DUMP_VRAM
#  endif

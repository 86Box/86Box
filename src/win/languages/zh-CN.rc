        MENUITEM "启用 IDE 日志\tCtrl+F8", IDM_LOG_IDE
# endif
# ifdef ENABLE_SERIAL_LOG
        MENUITEM "启用串口日志\tCtrl+F3", IDM_LOG_SERIAL
# endif
